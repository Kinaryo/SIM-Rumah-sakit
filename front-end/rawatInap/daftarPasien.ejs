<% layout('/layouts/app') %>
<style>
    /* Your existing styles */
</style>
<div style="margin-top: 125px;">
    <h2 style="text-align: center;">Daftar Pasien Rawat Inap</h2>
    <div class="tanggal">
        <h4 class="">Date: <span>Selasa, 25 Maret 2019</span></h4>
    </div>
    <table>
        <thead>
            <tr>
                <th>No Pendaftaran</th>
                <!-- Add additional headers based on your data model -->
            </tr>
        </thead>
        <tbody><% pasienRawatInap.forEach(pasien => { %>
            <tr>
                <% if (pasien.noPendaftaran && pasien.noPendaftaran.nama) { %>
                    <td><%= pasien.noPendaftaran.nama %></td>
                    <td><%= pasien.diagnosa %></td>
                    <!-- Add additional cells based on your data model -->
                <% } else { %>
                    <td>Data Tidak Tersedia</td>
                    <td>Data Tidak Tersedia</td>
                    <!-- Add additional cells based on your data model -->
                <% } %>
            </tr>
        <% }); %>
        
        </tbody>
    </table>
</div>
